{
  "root": true,
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "no-use-before-define": ["error", { "functions": false }],
    "no-unused-expressions": ["error", { "allowTaggedTemplates": true }]
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "extends": "airbnb"
}
